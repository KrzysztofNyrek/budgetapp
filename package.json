{
  "name": "budgetapp",
  "version": "1.0.0",
  "description": "Web app to calculate you mounth budget.",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "browser-sync start --server --files index.html css/*.css js/*.js --host 192.168.1.105",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss css/style.comp.css -u autoprefixer -b \"last 10 versions\" -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dekstryn/budgetapp.git"
  },
  "keywords": [
    "budget",
    "app",
    "webapp",
    "JS",
    "java",
    "script"
  ],
  "author": "Krzysztof Nyrek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dekstryn/budgetapp/issues"
  },
  "homepage": "https://github.com/Dekstryn/budgetapp#readme",
  "devDependencies": {
    "concat": "^1.0.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "autoprefixer": "^9.7.4",
    "postcss-cli": "^7.1.0"
  }
}
